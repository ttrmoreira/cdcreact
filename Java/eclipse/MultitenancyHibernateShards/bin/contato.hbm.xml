<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="java.magazine.shards.exemplo.modelo">
    <class name="Contato" table="CONTATO">
        <id name="contatoId" column="CONTATOID">
            <generator class="native"/>
        </id>
        <property name="inquilinoId" column="INQUILINOID"/>
        <property name="nome" column="NOME"/>
        <property name="telefone" column="TELEFONE"/>
        <property name="endereco" column="ENDERECO"/>
    </class>
</hibernate-mapping>
